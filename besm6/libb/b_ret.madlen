c---------------------------------------------------------------
c Restore registers from the stack.
c Called on exit from a B function.
c
     b/ret: ,name,
          6 ,mtj, 14
          7 ,mtj, 15
          7 ,stx, -4
            ,sti, 6
            ,sti, 7
            ,sti, 13
         14 ,mtj, 15
         13 ,uj,
            ,end,
